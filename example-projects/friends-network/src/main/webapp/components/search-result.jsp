<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<%@ taglib prefix="c" uri="jakarta.tags.core"%>
<%@ taglib prefix="app" tagdir="/WEB-INF/tags"%>


<c:forEach var="user" items="${users}">
	<div class="border mb-3 row py-2">
		<div class="col-2 align-self-center d-flex justify-content-center">
			<div class="profile-img-container align-self-center">
				<app:img src="profiles/${empty user.profile ? 'profile-photo.png' : user.profile}"/>
			</div>
		</div>
		<div class="col-auto">
			<div class="row mb-2">
				<div class="col">
					<span class="fw-bold">${empty user.name ? 'Name' : user.name}</span>
				</div>
			</div>
			<div class="row">
			<c:if test="${not empty user.thumbnails and 0 < user.thumbnails.size()}">
				<div class="col d-flex align-items-center">
					
					<div class="mutual-thumbnail">
						<img alt="" src="data:image/jpeg;base64,">
					</div>
					
					<div class="col ms-3">
						<small class="text-muted">${user.mutuals} mutual friends</small>
					</div>
				</div>
			</c:if>
			</div>
		</div>
		<div class="col-auto flex-fill text-end align-self-center">
			<button class="btn btn-outline-secondary">View</button>
			<c:choose>
				<c:when test="${user.status eq 'Friend'}">
					<form action="${pageContext.request.contextPath}/friends/delete" class="d-inline" method="post">
						<input name="id" value="${user.id}" type="hidden">
						<input name="keyword" value="${param.keyword}" type="hidden">
						<button class="btn btn-outline-danger">Unfriend</button>
					</form>
				</c:when>
				<c:when test="${user.status eq 'Inbox'}">
					<form action="${pageContext.request.contextPath}/friends/confirm" class="d-inline" method="post">
						<input name="id" value="${user.id}" type="hidden">
						<input name="keyword" value="${param.keyword}" type="hidden">
						<button class="btn btn-outline-primary">Confirm</button>
					</form>

					<form action="${pageContext.request.contextPath}/inboxes/delete" class="d-inline" method="post">
						<input name="id" value="${user.id}" type="hidden">
						<input name="keyword" value="${param.keyword}" type="hidden">
						<button class="btn btn-outline-danger">Delete</button>
					</form>
				</c:when>
				<c:when test="${user.status eq 'Requested'}">
					<form action="${pageContext.request.contextPath}/requests/cancel" class="d-inline" method="post">
						<input name="id" value="${user.id}" type="hidden">
						<input name="keyword" value="${param.keyword}" type="hidden">
						<button class="btn btn-outline-danger">Cancel</button>
					</form>
				</c:when>
				<c:otherwise>
					<c:url var="add" value="/requests/send"/>
					<form action="${add}" method="post" class="d-inline">
						<input name="id" value="${user.id}" type="hidden">
						<input name="keyword" value="${param.keyword}" type="hidden">
						<button type="submit" class="btn btn-outline-primary">Add</button>
					</form>
				</c:otherwise>
			</c:choose>
		</div>
	</div>
</c:forEach>